@model SplitAdminEcomerce.Models.Configurable

@{
    ViewData["Title"] = "Detalle configurable: " + Model.Descripcio;
    SplitAdminEcomerce.Models.Categoria categoria = (SplitAdminEcomerce.Models.Categoria)ViewBag.Categoria;
    SplitAdminEcomerce.Models.SubCategoria SubCategoria = (SplitAdminEcomerce.Models.SubCategoria)ViewBag.SubCategoria;
}
<div>
    <dl class="row">
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.Codigo)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.Codigo)
        </dd>
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.IdCategoria)
        </dt>
        <dd class="col-sm-10">
            @categoria.DescripcionFamilia
        </dd>
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.IdSubCategoria)
        </dt>
        <dd class="col-sm-10">
            @SubCategoria.Descripcion
        </dd>
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.Descripcio)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.Descripcio)
        </dd>
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.NivelConf)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.NivelConf)
        </dd>
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.Descuento)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.Descuento) %
        </dd>
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.Che_Activo)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.Che_Activo)
        </dd>
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.Che_IsProximamente)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.Che_IsProximamente)
        </dd>
    </dl>
</div>
