@model IEnumerable<SplitAdminEcomerce.Models.PrecioPatchCord>

<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Tipo)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Base)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Factor)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Tipo)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Base)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Factor)
            </td>
            <td>
                <a href="#modal2" data-toggle="modal" onclick="app_preciosConf.EditForm('PatchCord','@item.EncriptId','PatchCord')" title="Editar este componente">Editar</a>
            </td>
        </tr>
}
    </tbody>
</table>
