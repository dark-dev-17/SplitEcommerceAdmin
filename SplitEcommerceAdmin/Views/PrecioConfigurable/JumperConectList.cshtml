@model IEnumerable<SplitAdminEcomerce.Models.PrecioJumperConect>

<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.TipoJumper_)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.TipoFibra_)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.TipoCubierta_)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Precio)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.TipoJumper_)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.TipoFibra_)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.TipoCubierta_)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Precio)
            </td>
            <td>
                <a href="#modal2" data-toggle="modal" onclick="app_preciosConf.EditForm('JumperConect','@item.EncriptId','Jumper conector')" title="Editar este componente">Editar</a>
            </td>
        </tr>
}
    </tbody>
</table>
